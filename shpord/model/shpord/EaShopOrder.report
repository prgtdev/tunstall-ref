         <BLOCK_DEFINITION>BEGIN         
   :OpPrintQtyRequired := Iso_Unit_API.Convert_Unit_Quantity(:OpQtyRequired,:OpUnitMeas,:OpPrintUnit);
   
   IF part_ownership_API.encode(:OpCompPartOwnership) =&apos;COMPANY OWNED&apos; THEN
      :OpCompPartOwnerName           := NULL;
   ELSIF  part_ownership_API.encode(:OpCompPartOwnership) =&apos;CUSTOMER OWNED&apos;  THEN
      $IF Component_Order_SYS.INSTALLED $THEN
         :OpCompPartOwnerName := Cust_Ord_Customer_API.Get_Name(:OpCompPartOwnerId);
      $ELSE
         NULL;
      $END
   ELSIF  part_ownership_API.encode(:OpCompPartOwnership) =&apos;SUPPLIER LOANED&apos;  THEN
      $IF Component_Purch_SYS.INSTALLED $THEN
         :OpCompPartOwnerName  := SUPPLIER_API.Get_Vendor_Name(:OpCompPartOwnerId);
      $ELSE
         NULL;
      $END
   END IF;
   
EXCEPTION
WHEN OTHERS THEN
   :OpPrintQtyRequired := -1;
END;</BLOCK_DEFINITION>
